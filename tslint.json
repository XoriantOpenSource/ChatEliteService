{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "class-name": false,
        "object-literal-sort-keys": false,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "max-line-length": false,
        "no-var-requires": false,
        "no-empty": false,
        "no-console": [
            true,
            "log"
        ],
        "triple-equals": false
    },
    "rulesDirectory": []
}